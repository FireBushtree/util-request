{
  "private": false,
  "name": "envcloud-utils-request",
  "version": "0.0.2",
  "license": "MIT",
  "author": {
    "name": "FireBushtree"
  },
  "keywords": [
    "javascript",
    "download",
    "react-hook"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "build": "father-build",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "antd": "^4.15.5",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.0.14",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "react": "^16.12.0",
    "yorkie": "^2.0.0",
    "@types/qs": "^6.9.6"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "envcloud-utils-params": "^1.0.0",
    "qs": "^6.10.1"
  }
}
